{{ form_start(form) }}
<div class="row">
    <div class="col s12 m5">
        {{ form_row(form.name) }}
        {{ form_row(form.os) }}
        {{ form_row(form.location) }}
        {{ form_row(form.imageFile) }}
    </div>
    <div class="col s12 m5 offset-m2">
        {{ form_row(form.description) }}
    </div>
</div>
<button class="btn cyan darken-2">{{ button_label|default('Soumettre') }}</button>
{{ form_end(form) }}
